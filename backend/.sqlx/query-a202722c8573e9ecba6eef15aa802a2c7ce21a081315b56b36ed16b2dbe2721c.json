{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT f.id, f.parent_id, f.name, f.owner_id, f.created_at, f.updated_at\n        FROM folders f\n        JOIN permissions p ON p.folder_id IS NOT DISTINCT FROM f.id\n        WHERE p.user_id = $1\n          AND p.read = TRUE\n        ",
